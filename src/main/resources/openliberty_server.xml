<?xml version="1.0" encoding="UTF-8"?>
<server description="new server">
    <featureManager>
        <feature>javaee-8.0</feature>
        <feature>microProfile-3.0</feature>
    </featureManager>

    <basicRegistry
            id="basic"
            realm="BasicRealm">
    </basicRegistry>

    <httpEndpoint
            id="defaultHttpEndpoint"
            httpPort="9080"
            httpsPort="9443"
            host="*"/>
    <applicationManager
            autoExpand="true"/>

</server>
